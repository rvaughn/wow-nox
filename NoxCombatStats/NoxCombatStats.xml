<!--
  Nox Combat Stats

  Makes combat information available to the Nox Information Bar.

  @author Gruma
-->

<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
  <Script file="Localization.lua" />
  <Script file="NoxCombatStats.lua" />
  <Frame name="NoxCombatStatsFrame" parent="UIParent" hidden="true">
    <Scripts>
      <OnLoad>
        NoxCombatStats_OnLoad(self);
      </OnLoad>
      <OnEvent>
        NoxCombatStats_OnEvent(self, event, ...); 
      </OnEvent>
    </Scripts>
  </Frame>
</Ui>
