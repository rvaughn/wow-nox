<!--
  Nox Honor Stats

  Makes honor stats available to the Nox Information Bar. 

  @author Gruma
-->

<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
  <Script file="Localization.lua" />
  <Script file="NoxHonorStats.lua" />
  <Frame name="NoxHonorStatsFrame" parent="UIParent" hidden="true">
    <Scripts>
      <OnLoad>
        NoxHonorStats_OnLoad(self);
      </OnLoad>
      <OnEvent>
        NoxHonorStats_OnEvent(self, event, ...);
      </OnEvent>
    </Scripts>
  </Frame>
</Ui>
