<!--
  Nox Skill Stats

  Makes skill stats available to the Nox Information Bar. 

  @author Gruma
-->

<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/
..\FrameXML\UI.xsd">
  <Script file="Localization.lua" />
  <Script file="NoxSkillStats.lua" />
  <Frame name="NoxSkillStatsFrame" parent="UIParent" hidden="true">
    <Scripts>
      <OnLoad>
        NoxSkillStats_OnLoad(self);
      </OnLoad>
      <OnEvent>
        NoxSkillStats_OnEvent(self, event, ...);
      </OnEvent>
    </Scripts>
  </Frame>
</Ui>
